C：因为常数较小，$O(n^3)$暴枚即可通过，不用加任何优化

~~~{.cpp}
#include<bits/stdc++.h>
#define N 1111
#define CL(a) memset(a,0,sizeof a)
using namespace std;
int T,n,i,j,ca,an,L,R,cnt,c[N],a[N],v[N];
int main(){
	scanf("%d",&T);
	for(ca=1;ca<=T;ca++){
		scanf("%d",&n);an=0;
		for(i=1;i<=n;i++)scanf("%d",&a[i]),v[i]=a[i];
		sort(v+1,v+n+1);cnt=unique(v+1,v+n+1)-v-1;
		for(i=1;i<=n;i++)a[i]=lower_bound(v+1,v+cnt+1,a[i])-v;
		for(i=1;i<=n;i++){
			for(j=i;j<=n&&!c[a[j]];j++){
				c[a[j]]++;
				an=max(an,j-i+1);
				for(L=j+2,R=0;L<=n;L++){
					if(R<L)R=L;
					for(;R<=n&&!c[a[R]];)c[a[R++]]=1;
					an=max(an,j-i+1+R-L);
					if(R>L)c[a[L]]=0;
				}
			}
			for(;j>=i;j--)c[a[j]]=0;
		}
		printf("Case #%d: %d\n",ca,an);
	}
}
